  "bugs": {
    "url": "https://github.com/noriega3/next-stop-la-app/issues"
  },
  "homepage": "https://github.com/noriega3/next-stop-la-app#readme",
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "firebase": "^9.15.0",
    "gtfs-realtime-bindings": "^0.0.6",
    "node-fetch": "^3.3.0"
  }
}
